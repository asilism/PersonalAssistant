# LLM Provider: anthropic, openai, or openrouter
LLM_PROVIDER=anthropic

# API Keys (set the one you're using)
ANTHROPIC_API_KEY=your_api_key_here
OPENAI_API_KEY=your_openai_key_here
OPENROUTER_API_KEY=your_openrouter_key_here

# LLM Model (depends on provider)
# For Anthropic: claude-3-5-sonnet-20241022, claude-3-opus-20240229, etc.
# For OpenAI: gpt-4-turbo-preview, gpt-4, gpt-3.5-turbo, etc.
# For OpenRouter: anthropic/claude-3.5-sonnet, openai/gpt-4-turbo, etc.
LLM_MODEL=claude-3-5-sonnet-20241022

# System Configuration
MAX_RETRIES=3
TIMEOUT=30000
